@font-face {
  font-family: '12306';
  src:  url('../fonts/12306.eot?pcrovc');
  src:  url('../fonts/12306.eot?pcrovc#iefix') format('embedded-opentype'),
    url('../fonts/12306.ttf?pcrovc') format('truetype'),
    url('../fonts/12306.woff?pcrovc') format('woff'),
    url('../fonts/12306.svg?pcrovc#12306') format('svg');
  font-weight: normal;
  font-style: normal;
  font-display: block;
}

[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: '12306' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

